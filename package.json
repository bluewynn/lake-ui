{
  "name": "lake-ui",
  "version": "1.0.0",
  "description": "Vue mobile UI components",
  "keywords": [
    "vue",
    "UI"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:bluewynn/lake-ui.git",
  "author": "bluewynn",
  "license": "MIT",
  "scripts": {
    "dev:demo": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "build:lib": "rimraf lib && cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build:demo": "cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "lint": "eslint src/**/*.vue,src/**/*.js",
    "lint:format": "yarn lint && prettier --write src/**/*.vue,src/**/*.js"
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=5.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "^9.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cache-loader": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.2.1",
    "svgo-loader": "^2.2.0",
    "thread-loader": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
